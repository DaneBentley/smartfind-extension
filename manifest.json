{
  "manifest_version": 3,
  "name": "SmartFind: AI-Enhanced Search",
  "version": "1.0.0",
  "description": "Replace Ctrl+F with AI-enhanced search. Ask natural questions like 'Where is contact info?' or use exact keywords. Get intelligent highlighting and instant results on any webpage.",
  "author": "Dane Bentley",
  "homepage_url": "https://smartfind-api.vercel.app",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "identity",
    "webNavigation",
    "alarms"
  ],
  "host_permissions": [
    "https://smartfind-api.vercel.app/*",
    "https://accounts.google.com/*",
    "https://*.supabase.co/*"
  ],
  "oauth2": {
    "client_id": "148916188620-nln6nq5kn23hm435g0i7mrs72becjqb2.apps.googleusercontent.com",
    "scopes": ["email", "profile"]
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "action": {
    "default_title": "SmartFind: AI-Enhanced Search",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "data:image/svg+xml;base64,",
      "32": "data:image/svg+xml;base64,",
      "48": "data:image/svg+xml;base64,",
      "128": "data:image/svg+xml;base64,"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "auth.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://smartfind-api.vercel.app https://accounts.google.com https://*.supabase.co"
  },
  "commands": {
    "toggle-smartfind": {
      "suggested_key": {
        "default": "Ctrl+F",
        "mac": "Command+F",
        "windows": "Ctrl+F",
        "linux": "Ctrl+F"
      },
      "description": "Activate SmartFind AI-enhanced search"
    }
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "32": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  },
  "minimum_chrome_version": "88",
  "short_name": "SmartFind"
}